{
    "name": "vulcangz/yii2-psr7-bridge",
    "description": "A PSR7 Bridge for Yii2",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Charles R. Portwood II",
            "email": "charlesportwoodii@erianna.com"
        },
        {
            "name": "vulcangz",
            "email": "zhugd168@163.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "yiisoft/yii2": "^2.0.15",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "nyholm/psr7": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0.2",
        "samdark/yii2-psr-log-target": "^1.0",
        "monolog/monolog": "^1.24",
        "squizlabs/php_codesniffer": "^3.2",
        "spiral/roadrunner": "^1.2"
    },
    "autoload": {
        "psr-4": { "yii\\Psr7\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "yii\\Psr7\\tests\\": "tests/" }
    }
}
